{
  "$schema": "../schema/dnslint.schema.json#",
  "aws": {
    "enabled": true
  },
  "domains": [
    {
      "enabled": true,
      "name": "*",
      "records": [
        {
          "timeout": 1000,
          "type": "*"
        },
        {
          "enabled": true,
          "type": "A",
          "webcalls":
          [
            {
              "callTimeMax": 200,
              "headers": [
                {
                  "name": "location",
                  "value": {
                    "disposition": "required"
                  }
                }
              ],
              "protocol": "http",
              "statusCode": 301
            },
            {
              "callTimeMax": 200,
              "headers": [
                {
                  "name": "content-type",
                  "value": {
                    "disposition": "required"
                  }
                },
                {
                  "name": "content-security-policy",
                  "value": {
                    "disposition": "required",
                    "filter": "object-src 'none'"
                  }
                },
                {
                  "name": "x-content-type-options",
                  "value": {
                    "disposition": "required",
                    "filter": "nosniff"
                  }
                },
                {
                  "name": "x-frame-options",
                  "value": {
                    "disposition": "required",
                    "filter": "DENY"
                  }
                },
                {
                  "name": "x-xss-protection",
                  "value": {
                    "disposition": "required",
                    "filter": "1; mode=block"
                  }
                },
                {
                  "name": "referrer-policy",
                  "value": {
                    "disposition": "required",
                    "filter": "same-origin"
                  }
                }
              ],
              "protocol": "https",
              "statusCode": 200
            }
          ]
        },
        {
          "enabled": true,
          "type": "AAAA",
          "webcalls":
          [
            {
              "headers": [
                {
                  "name": "location",
                  "value": {
                    "disposition": "required"
                  }
                }
              ],
              "protocol": "http",
              "statusCode": 301
            },
            {
              "headers": [
                {
                  "name": "content-type",
                  "value": {
                    "disposition": "required"
                  }
                },
                {
                  "name": "content-security-policy",
                  "value": {
                    "disposition": "required",
                    "filter": "object-src 'none'"
                  }
                },
                {
                  "name": "x-content-type-options",
                  "value": {
                    "disposition": "required",
                    "filter": "nosniff"
                  }
                },
                {
                  "name": "x-frame-options",
                  "value": {
                    "disposition": "required",
                    "filter": "DENY"
                  }
                },
                {
                  "name": "x-xss-protection",
                  "value": {
                    "disposition": "required",
                    "filter": "1; mode=block"
                  }
                },
                {
                  "name": "referrer-policy",
                  "value": {
                    "disposition": "required",
                    "filter": "same-origin"
                  }
                }
              ],
              "protocol": "https",
              "statusCode": 200
            }
          ]
        },
        {
          "enabled": true,
          "inheritable": true,
          "type": "CAA",
          "values": [
            {
              "disposition": "required",
              "filter": {
                "flags": 0,
                "issuerCritical": false,
                "tag": "^issue$",
                "value": "^amazon.com$"
              }
            },
            {
              "disposition": "required",
              "filter": {
                "flags": 0,
                "issuerCritical": false,
                "tag": "^issue$",
                "value": "^amazonaws.com$"
              }
            },
            {
              "disposition": "required",
              "filter": {
                "flags": 0,
                "issuerCritical": false,
                "tag": "^issue$",
                "value": "^amazontrust.com$"
              }
            },
            {
              "disposition": "required",
              "filter": {
                "flags": 0,
                "issuerCritical": false,
                "tag": "^issue$",
                "value": "^awstrust.com$"
              }
            }
          ]
        },
        {
          "enabled": true,
          "type": "CNAME",
          "webcalls":
          [
            {
              "headers": [
                {
                  "name": "location",
                  "value": {
                    "disposition": "required"
                  }
                }
              ],
              "protocol": "http",
              "statusCode": 301
            },
            {
              "headers": [
                {
                  "name": "content-type",
                  "value": {
                    "disposition": "required"
                  }
                },
                {
                  "name": "content-security-policy",
                  "value": {
                    "disposition": "required",
                    "filter": "object-src 'none'"
                  }
                },
                {
                  "name": "x-content-type-options",
                  "value": {
                    "disposition": "required",
                    "filter": "nosniff"
                  }
                },
                {
                  "name": "x-frame-options",
                  "value": {
                    "disposition": "required",
                    "filter": "DENY"
                  }
                },
                {
                  "name": "x-xss-protection",
                  "value": {
                    "disposition": "required",
                    "filter": "1; mode=block"
                  }
                },
                {
                  "name": "referrer-policy",
                  "value": {
                    "disposition": "required",
                    "filter": "same-origin"
                  }
                }
              ],
              "protocol": "https",
              "statusCode": 200
            }
          ]
        },
        {
          "enabled": true,
          "type": "MX"
        },
        {
          "enabled": true,
          "type": "TXT"
        }
      ],
      "validations": [
        {

        }
      ]
    }
  ],
  "servers": [
    {
      "filter": [
        "^api\\.",
        "^devapi\\."
      ],
      "name": "api",
      "records": [
        {
          "type": "A|CNAME"
        },
        {
          "type": "A"
        },
        {
          "type": "CNAME"
        },
        {
          "type": "TXT"
        }
      ]
    },
    {
      "filter": [
        "._domainkey."
      ],
      "name": "domainkey",
      "records": [
        {
          "type": "A",
          "webcalls":
          [
            {
              "enabled": false,
              "protocol": "http"
            },
            {
              "enabled": false,
              "protocol": "https"
            }
          ]
        },
        {
          "type": "AAAA",
          "webcalls":
          [
            {
              "enabled": false,
              "protocol": "http"
            },
            {
              "enabled": false,
              "protocol": "https"
            }
          ]
        },
        {
          "type": "CNAME",
          "webcalls":
          [
            {
              "enabled": false,
              "protocol": "http"
            },
            {
              "enabled": false,
              "protocol": "https"
            }
          ]
        },
        {
          "type": "TXT",
          "values": [
            {
              "disposition": "required",
              "filter": "^v=DKIM1; "
            }
          ]
        }
      ]
    },
    {
      "filter": [
        "^_dmarc."
      ],
      "name": "dmarc",
      "records": [
        {
          "type": "TXT",
          "values": [
            {
              "disposition": "required",
              "filter": "^v=DMARC1; "
            }
          ]
        }
      ]
    },
    {
      "*": true,
      "filters": [
        "^www\\."
      ],
      "name": "website",
      "records": [
        {
          "type": "A|CNAME"
        },
        {
          "type": "A"
        },
        {
          "type": "CNAME"
        },
        {
          "type": "MX",
          "values": [{
              "disposition": "required",
              "filter": {
                "exchange": "alt1.aspmx.l.google.com",
                "preference": 5
              }
            }
          ]
        },
        {
          "type": "TXT"
        }
      ],
      "validations": {
        "crawl": [
          {
            "enabled": true
          }
        ],
        "favicon": [
          {
            "enabled": true,
            "files": [
              {
                "disposition": "required",
                "path": "/favicon.ico"
              }
            ]
          }
        ],
        "robots": [
          {
            "enabled": true,
            "files": [
              {
                "disposition": "required",
                "path": "/robots.txt"
              }
            ]
          }
        ],
        "scripts": [
          {
            "enabled": true,
            "tags": [
              {
                "disposition": "required",
                "tags": [
                  {
                    "disposition": "required",
                    "filter": "",
                    "path": "/html/head/script"
                  }
                ]
              }
            ]
          }
        ],
        "seo": [
          {
            "enabled": true
          }
        ],
        "sitmap": [
          {
            "enabled": true,
            "files": [
              {
                "disposition": "required",
                "path": "/sitemap.xml"
              }
            ]
          }
        ],
        "social": [
          {
            "enabled": true,
            "files": [
              {
                "disposition": "required",
                "path": "/",
                "tags": [
                  {
                    "disposition": "required",
                    "path": "/html/head/meta[@property='og:url']"
                  },
                  {
                    "disposition": "required",
                    "path": "/html/head/meta[@property='og:title']"
                  },
                  {
                    "disposition": "required",
                    "path": "/html/head/meta[@property='og:description']"
                  },
                  {
                    "disposition": "required",
                    "path": "/html/head/meta[@property='og:image']"
                  },
                  {
                    "disposition": "required",
                    "path": "/html/head/meta[@property='fb:app_id']"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}